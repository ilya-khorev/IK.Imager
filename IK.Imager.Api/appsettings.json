{
  "Logging": {
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Error",
        "System": "Error"
      }
    },
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Error",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "ApplicationInsights": {
    "EnableDependencyTrackingTelemetryModule": false,
    "AuthenticationApiKey": ""
  },
  "ServiceBus": {
    "ConnectionString": ""
  },
  "ImageLimitations" : {
    "Width": {
      "Min": 60,
      "Max": 3000
    },
    "Height": {
      "Min": 60,
      "Max": 3000
    },
    "SizeBytes": {
      "Min": 100000,
      "Max": 15000000
    },
    "Types": ["JPEG, PNG, GIF, BMP"]
  },
  
  "AllowedHosts": "*"
}
